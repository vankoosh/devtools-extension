{
  "name": "Add CSS to Block Elements",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": {
      "16": "star.png"
    },
    "default_title": "Apply CSS",
    "default_popup": "app.html"
  },
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}